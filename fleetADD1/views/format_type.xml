<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="typ_format_list_view">
      <field name="name">typ format list</field>
      <field name="model">typformat</field>
      <field name="arch" type="xml">
        <tree editable ="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="typf_ormat_form_view">
      <field name="name">typ format form</field>
      <field name="model">typformat</field>
      <field name="arch" type="xml">
        <form>
          <group>
            <field name="name"/>
          </group>
        </form>
      </field>
    </record>
</data>
</odoo>